{
  "name": "youtube-cutter",
  "version": "1.0.5",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "package-mac": "rm -rf ./node_modules && npm build && electron-builder build --mac",
    "package-linux": "rm -rf ./node_modules && npm build && electron-builder build --linux",
    "package-win": "rm -rf ./node_modules && npm build && electron-builder build --win --x64",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "website:start": "npm-run-all website:build:css --parallel website:start:*",
    "website:start:serve": "serve ./website --listen 3000",
    "website:start:ts": "tsc --project ./website --watch",
    "website:build": "run-p website:build:*",
    "website:build:css": "cp ./node_modules/minireset.css/minireset.min.css ./website/styles/",
    "website:build:ts": "tsc --project ./website"
  },
  "main": "background.js",
  "dependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/i18n": "^0.12.0",
    "@types/lodash": "^4.14.157",
    "@types/node": "12.12.6",
    "@types/winston": "^2.4.4",
    "@types/youtube": "^0.0.41",
    "@types/youtube-player": "^5.5.3",
    "core-js": "^3.6.5",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.8",
    "ffmpeg-static": "^4.2.5",
    "flag-icon-css": "^3.5.0",
    "fluent-ffmpeg": "^2.1.2",
    "i18n": "^0.13.2",
    "lodash": "^4.17.19",
    "material-design-icons-iconfont": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "sanitize-filename": "^1.6.3",
    "tsyringe": "^4.3.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.22.4",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vuetify": "^2.4.4",
    "vuex": "^3.6.0",
    "vuex-class": "^0.3.2",
    "winston": "^3.3.3",
    "youtube-player": "^5.5.2",
    "ytdl-core": "^4.8.3",
    "ytpl": "^2.0.4"
  },
  "devDependencies": {
    "@types/ffmpeg-static": "^3.0.0",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "electron": "^13.6.6",
    "electron-devtools-installer": "^3.1.1",
    "electron-icon-builder": "^1.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "serve": "^11.3.2",
    "spectron": "^11.1.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "~3.9.3",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.4",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  }
}
